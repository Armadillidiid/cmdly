{
	"name": "cmd-sage",
	"version": "0.0.0",
	"type": "module",
	"packageManager": "pnpm@10.14.0",
	"license": "MIT",
	"description": "",
	"bin": {
		"cmd-sage": "./dist/bin.cjs"
	},
	"exports": {
		"./bin": {
			"require": "./dist/bin.cjs"
		}
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Armadillidiid/cmd-sage"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=23"
	},
	"scripts": {
		"build": "node scripts/build.ts",
		"build:watch": "node scripts/build.ts --watch",
		"prepare": "effect-language-service patch",
		"clean": "rimraf dist/*",
		"typecheck": "tsc -b tsconfig.json",
		"test": "vitest run",
		"changeset-version": "changeset version",
		"changeset-publish": "pnpm build && pnpm vitest && changeset publish && changeset tag && git push --tags",
		"format": "biome format --write",
		"lint": "biome lint --write",
		"check": "biome check --write --unsafe"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@changesets/changelog-github": "^0.5.2",
		"@changesets/cli": "^2.29.8",
		"@effect/cli": "^0.72.1",
		"@effect/language-service": "latest",
		"@effect/platform": "^0.93.6",
		"@effect/platform-node": "^0.103.0",
		"@effect/vitest": "^0.27.0",
		"@types/copy-paste": "^2.1.0",
		"@types/node": "^24.10.1",
		"effect": "^3.19.9",
		"pkgroll": "^2.21.4",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^2.0.53",
		"@ai-sdk/google": "^2.0.44",
		"@ai-sdk/openai": "^2.0.80",
		"@ai-sdk/openai-compatible": "^1.0.28",
		"@shikijs/cli": "^3.19.0",
		"ai": "^5.0.108",
		"copy-paste": "^2.2.0",
		"shiki": "3.19.0"
	}
}
